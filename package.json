{
  "name": "react-mui-src-starter-kit",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest --reporter=json --outputFile=./test-results.json --watch=false",
    "test:coverage": "vitest run --coverage",
    "_comment_start": "Recommended script for local dev. `npm run start:dev` will start the frontend on PORT value (4000) and the backend on BACKEND_PORT(4001). The backend should be using a self-signed certificate for HTTPS.",
    "start:dev": "cd src && cross-env PORT=4000 HTTPS=true BACKEND_PORT=4001 USE_LOCAL=true npm run dev",
    "_comment": "The following scripts are for local development only to quickly bypass the pipeline for testing things that might only be occuring in a given environment. They are not intended for use in a CI/CD pipeline.",
    "deploy:dev": "aws s3 sync ./dist/ s3://static.generic.lcwpp.dev.timmons-dev.com/lcwpp",
    "deploy:qa": "aws s3 sync ./dist/ s3://static.generic.lcwpp.qa.timmons-dev.com/lcwpp",
    "deploy:uat": "aws s3 sync ./dist/ s3://static.generic.lcwpp.uat.timmons-dev.com/lcwpp"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@timmons-group/config-form": "^2.0.0-beta.8",
    "@timmons-group/shared-auth-config": "^2.0.0-beta.8",
    "@timmons-group/shared-react-app-bar": "^2.0.0-beta.8",
    "@timmons-group/shared-react-auth": "^2.0.0-beta.8",
    "@timmons-group/shared-react-components": "^2.0.0-beta.8",
    "@timmons-group/shared-react-permission-filter": "^2.0.0-beta.8",
    "axios": "^1.7.7",
    "axios-retry": "3.9.1",
    "buffer": "^6.0.3",
    "date-fns": "^2.30.0",
    "decimal.js": "^10.4.3",
    "notistack": "^2.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "yup": "^1.4.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@mui/x-date-pickers": "^6.20.2",
    "@tanstack/react-query": "^5.56.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.6.1",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "jsdom": "^24.1.3",
    "material-react-table": "^2.13.3",
    "react-router-dom": "^6.26.2",
    "sass": "^1.79.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-mkcert": "^1.17.6",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vitest": "^2.1.1"
  }
}
